<flowgorithm fileversion="4.2">
    <attributes>
        <attribute name="name" value="ee"/>
        <attribute name="authors" value="nikol"/>
        <attribute name="about" value=""/>
        <attribute name="saved" value="2024-10-29 03:21:07 AM"/>
